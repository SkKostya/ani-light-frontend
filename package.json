{
  "name": "ani-light-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build:prod": "rsbuild build",
    "build:dev": "cross-env NODE_ENV=development rsbuild build",
    "preview": "rsbuild preview",
    "lint": "eslint .",
    "lint:fix-css": "npx stylelint \"**/*.(css|scss|sass)\" --fix",
    "prettier:fix": "prettier --write --ignore-unknown",
    "knip": "knip",
    "prepare": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@reduxjs/toolkit": "^2.9.0",
    "artplayer": "^5.3.0",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "hls.js": "^1.6.13",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^16.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.9.3",
    "universal-cookie": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@rsbuild/core": "^1.5.6",
    "@rsbuild/plugin-node-polyfill": "^1.4.2",
    "@rsbuild/plugin-react": "^1.4.0",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rsbuild/plugin-svgr": "^1.2.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "globals": "^16.4.0",
    "knip": "^5.64.1",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "./**/*.{css,scss,sass}": [
      "npm run lint:fix-css"
    ],
    "./**/*": [
      "npm run prettier:fix"
    ]
  },
  "knip": {
    "entry": [
      "src/index.{ts,tsx}"
    ],
    "project": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
